{
  "name": "@paragrid/web",
  "version": "1.0.0",
  "description": "Paragrid TypeScript implementation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:clean && tsc && npm run build:index && npm run build:demo",
    "build:clean": "rm -rf dist",
    "build:index": "vite build --outDir dist --emptyOutDir false",
    "build:demo": "BUILD_TARGET=demo vite build --outDir dist --emptyOutDir false",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "paragrid",
    "visualization",
    "grid"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "fraction.js": "^5.3.4",
    "iso-render": "file:../../ts-poly"
  }
}
